:css
  body {
    padding-top: 20px;
    padding-bottom: 20px;
  }
  .navbar {
    margin-bottom: 20px;
  }

%nav.navbar.navbar-default
  %div.container-fluid
    %div.navbar-header
      %button.navbar-toggle.collapsed{"type"=>"button", "data-toggle"=>"collapse", "data-target"=>"#navbar", "aria-expanded"=>"false", "aria-controls"=>"navbar"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      %a.navbar-brand(href="/") Smart-Form
    %div#navbar.navbar-collapse.collapse
      %ul.nav.navbar-nav
        %li= link_to "Smart-forms", list_smart_forms_path
        %li= link_to "Category", categories_path
        %li= link_to "Sub Category", sub_categories_path
        %li
          %a( href="https://github.com/altherlex/smart_form" target="_blank") Github